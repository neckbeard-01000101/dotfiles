import = ["~/.config/alacritty/catppuccin-mocha.toml"]
[window]
decorations = "none"
padding = {x = 10, y = 10}
startup_mode = "Maximized"
[font]
size = 12
[font.bold]
family = "JetBrainsMono Nerd Font"
style = "Bold"

[font.bold_italic]
family = "JetBrainsMono Nerd Font"
style = "Bold Italic"

[font.italic]
family = "JetBrainsMono Nerd Font"
style = "Italic"

[font.normal]
family = "JetBrainsMono Nerd Font"
style = "Regular"
[env]
TERM = "xterm-256color"
